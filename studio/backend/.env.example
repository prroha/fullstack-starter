# Database
DATABASE_URL="postgresql://user:password@localhost:5432/starter_studio?schema=public"

# Server
PORT=3001
NODE_ENV=development

# JWT
JWT_SECRET=your-super-secret-jwt-key-change-in-production
JWT_ACCESS_EXPIRES_IN=15m
JWT_REFRESH_EXPIRES_IN=7d

# Cors
CORS_ORIGIN=http://localhost:3002

# Stripe
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...
STRIPE_PUBLISHABLE_KEY=pk_test_...
STRIPE_SUCCESS_URL=http://localhost:3002/checkout/success?session_id={CHECKOUT_SESSION_ID}
STRIPE_CANCEL_URL=http://localhost:3002/checkout?cancelled=true

# Email (Resend)
RESEND_API_KEY=re_...
EMAIL_FROM=noreply@starterstudio.com

# Storage (S3/R2)
S3_ENDPOINT=
S3_BUCKET=
S3_ACCESS_KEY=
S3_SECRET_KEY=
S3_REGION=auto

# Admin
ADMIN_EMAIL=admin@starterstudio.com
ADMIN_PASSWORD=changeme
